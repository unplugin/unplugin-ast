{
  "name": "unplugin-ast",
  "version": "0.7.0",
  "packageManager": "pnpm@8.1.0",
  "description": "Manipulate the AST to transform your code.",
  "keywords": [
    "unplugin",
    "rollup",
    "vite",
    "esbuild",
    "webpack"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-ast#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-ast/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-ast.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./transformers": {
      "types": "./dist/transformers.d.ts",
      "require": "./dist/transformers.js",
      "import": "./dist/transformers.mjs"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json,.md",
    "lint:fix": "pnpm run lint -- --fix",
    "build": "tsup && tsx scripts/postbuild.mts",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.2",
    "@babel/generator": "^7.21.4",
    "@babel/parser": "^7.21.4",
    "@rollup/pluginutils": "^5.0.2",
    "magic-string": "^0.30.0",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "@babel/types": "^7.21.4",
    "@sxzz/eslint-config": "^2.4.6",
    "@types/babel__generator": "^7.6.4",
    "@types/node": "^18.15.11",
    "bumpp": "^9.1.0",
    "eslint": "^8.37.0",
    "eslint-define-config": "^1.17.0",
    "estree-walker": "^3.0.3",
    "fast-glob": "^3.2.12",
    "prettier": "^2.8.7",
    "rollup": "^3.20.2",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
