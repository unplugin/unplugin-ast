{
  "name": "unplugin-ast",
  "version": "0.5.5",
  "packageManager": "pnpm@7.11.0",
  "description": "Manipulate the AST to transform your code.",
  "keywords": [
    "unplugin",
    "rollup",
    "vite",
    "esbuild",
    "webpack"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-ast#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-ast/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-ast.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./transformers": {
      "require": "./dist/transformers.js",
      "import": "./dist/transformers.mjs"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json,.md",
    "lint:fix": "pnpm run lint -- --fix",
    "build": "tsup && tsx scripts/postbuild.mts",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@antfu/utils": "^0.5.2",
    "@babel/parser": "^7.19.0",
    "@rollup/pluginutils": "^4.2.1",
    "magic-string": "^0.26.3",
    "unplugin": "^0.9.5"
  },
  "devDependencies": {
    "@babel/generator": "^7.19.0",
    "@babel/types": "^7.19.0",
    "@sxzz/eslint-config": "^2.4.3",
    "@types/babel__generator": "^7.6.4",
    "@types/node": "*",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.0",
    "eslint-define-config": "^1.7.0",
    "estree-walker": "^3.0.1",
    "fast-glob": "^3.2.12",
    "prettier": "^2.7.1",
    "rollup": "^2.79.0",
    "tsup": "^6.2.3",
    "tsx": "^3.9.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vitest": "^0.23.2"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
