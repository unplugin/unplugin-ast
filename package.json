{
  "name": "unplugin-ast",
  "version": "0.8.2",
  "packageManager": "pnpm@8.6.11",
  "description": "Manipulate the AST to transform your code.",
  "type": "module",
  "keywords": [
    "unplugin",
    "rollup",
    "vite",
    "esbuild",
    "webpack"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-ast#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-ast/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-ast.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/index.d.cts",
        "import": "./dist/index.d.ts"
      },
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": {
        "require": "./dist/vite.d.cts",
        "import": "./dist/vite.d.ts"
      },
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "types": {
        "require": "./dist/webpack.d.cts",
        "import": "./dist/webpack.d.ts"
      },
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "types": {
        "require": "./dist/rollup.d.cts",
        "import": "./dist/rollup.d.ts"
      },
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "types": {
        "require": "./dist/esbuild.d.cts",
        "import": "./dist/esbuild.d.ts"
      },
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./transformers": {
      "types": {
        "require": "./dist/transformers.d.cts",
        "import": "./dist/transformers.d.ts"
      },
      "require": "./dist/transformers.cjs",
      "import": "./dist/transformers.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.5",
    "@babel/generator": "^7.22.9",
    "@babel/parser": "^7.22.7",
    "@rollup/pluginutils": "^5.0.2",
    "ast-kit": "^0.9.4",
    "magic-string-ast": "^0.3.0",
    "unplugin": "^1.4.0"
  },
  "devDependencies": {
    "@babel/types": "^7.22.5",
    "@sxzz/eslint-config": "^3.2.3",
    "@sxzz/prettier-config": "^1.0.4",
    "@types/babel__generator": "^7.6.4",
    "@types/node": "^20.4.5",
    "bumpp": "^9.1.1",
    "eslint": "^8.46.0",
    "eslint-define-config": "^1.22.0",
    "fast-glob": "^3.3.1",
    "prettier": "^3.0.0",
    "rollup": "^3.27.0",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@sxzz/prettier-config"
}
